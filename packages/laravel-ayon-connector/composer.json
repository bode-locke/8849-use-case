{
    "name": "benjamin/laravel-ayon-connector",
    "description": "Laravel package pour synchroniser avec AYON",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Votre Nom",
            "email": "votre@email.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Benjamin\\AyonConnector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Benjamin\\AyonConnector\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Benjamin\\AyonConnector\\AyonConnectorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "orchestra/testbench": "^10.6"
    }
}
